{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "SSMReadParam",
      "Effect": "Allow",
      "Action": [
        "ssm:GetParameter",
        "ssm:GetParameters"
      ],
      "Resource": "arn:aws:ssm:ap-southeast-2:382467720155:parameter/lumairej/prod/*"
    },
    {
      "Sid": "KMSDecryptForSSM",
      "Effect": "Allow",
      "Action": "kms:Decrypt",
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "kms:ViaService": "ssm.ap-southeast-2.amazonaws.com",
          "kms:CallerAccount": "382467720155"
        }
      }
    }
  ]
}
